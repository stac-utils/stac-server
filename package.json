{
  "displayName": "stac-server",
  "description": "A STAC API running on stac-server",
  "version": "0.5.1",
  "repository": "https://github.com/stac-utils/stac-server",
  "author": "Alireza Jazayeri, Matthew Hanson <matt.a.hanson@gmail.com>, Sean Harkins",
  "license": "MIT",
  "scripts": {
    "build": "./bin/build.sh",
    "lint": "eslint . --ext .js",
    "lint-js-fix": "eslint . --ext .js --fix",
    "check-openapi": "spectral lint src/lambdas/api/openapi.yaml --ruleset .spectral.yml",
    "test": "npm run test:unit",
    "test:coverage": "c8 npm run test:unit",
    "test:integration": "ava tests/integration/*.js",
    "test:system": "./bin/system-tests.sh",
    "test:unit": "ava tests/unit/*.js",
    "typecheck": "tsc",
    "audit-prod": "npm audit --omit dev",
    "deploy": "sls deploy",
    "package": "sls package",
    "serve": "STAC_API_URL=http://localhost:3000 ENABLE_TRANSACTIONS_EXTENSION=true nodemon ./src/lambdas/api/local.js",
    "build-api-docs": "npm run widdershins --search false --language_tabs 'nodejs:NodeJS' 'python:Python' --summary ./packages/api-lib/api-spec.yaml -o ./docs/api.md & npm run shins --inline --logo ./docs/images/logo.png -o ./docs/index.html ./docs/api.md"
  },
  "pre-commit": {
    "silent": false,
    "run": [
      "lint",
      "typecheck",
      "check-openapi",
      "audit-prod",
      "test",
      "test:system",
      "build"
    ]
  },
  "ava": {
    "verbose": true
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/stac-utils/stac-server/issues"
  },
  "dependencies": {
    "@acuris/aws-es-connection": "^1.1.0",
    "@elastic/elasticsearch": "^7.9.0",
    "@mapbox/extent": "^0.4.0",
    "@opensearch-project/opensearch": "^2.1.0",
    "aws-os-connection": "^0.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "got": "^11.8.5",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "memorystream": "^0.3.1",
    "morgan": "^1.10.0",
    "p-filter": "^3.0.0",
    "pump": "^3.0.0",
    "serverless-http": "^3.1.0",
    "through2": "^4.0.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@stoplight/spectral-cli": "^6.6.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/aws-lambda": "^8.10.109",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/http-errors": "^2.0.1",
    "@types/luxon": "^3.1.0",
    "@types/memorystream": "^0.3.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@types/sinon": "^10.0.13",
    "ava": "^5.1.0",
    "aws-event-mocks": "^0.0.0",
    "aws-sdk": "^2.1277.0",
    "c8": "^7.12.0",
    "copy-webpack-plugin": "^11.0.0",
    "crypto-random-string": "^3.0.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "luxon": "^3.1.1",
    "nock": "^13.2.9",
    "nodemon": "^2.0.20",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.1",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "proxyquire": "^2.1.3",
    "serverless": "^3.25.1",
    "serverless-offline": "^12.0.3",
    "shins": "^2.6.0",
    "sinon": "^15.0.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "widdershins": "^4.0.1",
    "zip-webpack-plugin": "^4.0.1"
  },
  "name": "stac-server"
}
